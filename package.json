{
    "name": "simple-units",
    "version": "1.1.5",
    "description": "unit converter with focus on the golden mean between simplicity, customizability and performance",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "type": "module",
    "scripts": {
        "watch": "npx tsc --watch",
        "build": "npx tsc",
        "start": "cd ./dist && node",
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
        "docs": "npx typedoc --out docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/harrydehix/simple-units.git"
    },
    "keywords": [
        "simple-units",
        "units",
        "unit converter",
        "simple-units",
        "weather",
        "SI units",
        "US units"
    ],
    "author": "Henri Hollmann",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/harrydehix/simple-units/issues"
    },
    "homepage": "https://github.com/harrydehix/simple-units#readme",
    "devDependencies": {
        "@gribnoysup/wunderbar": "^2.2.2",
        "@types/convert-units": "^2.3.4",
        "@types/node": "^15.14.0",
        "@types/source-map-support": "^0.5.4",
        "@types/sprintf-js": "^1.1.2",
        "@typescript-eslint/eslint-plugin": "^4.28.1",
        "@typescript-eslint/parser": "^4.28.1",
        "convert": "^4.1.3",
        "convert-units": "^2.3.4",
        "eslint": "^7.29.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^10.0.0",
        "typedoc": "^0.26.7",
        "typescript": "^5.7.0-dev.20240928",
        "units-converter": "^1.0.3"
    },
    "dependencies": {
        "cartesian-product-generator": "^1.1.1",
        "sprintf-js": "^1.1.2"
    }
}
